{
  "name": "react-colors-beauty",
  "version": "1.0.0",
  "description": "颜色选择器，同时支持纯色和渐变色。",
  "files": [
    "esm",
    "es2017",
    "cjs",
    "dist"
  ],
  "main": "esm/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "es2017": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      },
      "default": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint"
  },
  "keywords": [
    "ice",
    "react",
    "component"
  ],
  "dependencies": {
    "@ice/jsx-runtime": "^0.2.0",
    "@rc-component/color-picker": "~1.4.1",
    "@swc/helpers": "^0.5.1",
    "rc-input": "~1.4.2",
    "rc-input-number": "~8.6.1"
  },
  "devDependencies": {
    "@applint/spec": "^1.2.3",
    "@ice/pkg": "^1.0.0",
    "@ice/pkg-plugin-docusaurus": "^1.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^15.0.0"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleepy-zone/react-colors.git"
  },
  "author": "sleepy__zone",
  "bugs": {
    "url": "https://github.com/sleepy-zone/react-colors/issues"
  },
  "homepage": "https://github.com/sleepy-zone/react-colors#readme"
}
